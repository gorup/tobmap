namespace tobmapsnap;

table SnapBucket {
   cell_id:uint64;
   
   // Parellel arrays, first is sorted so you can binary search it, then you look at the second
   // to know which edge it is.
   edge_cell_ids:[uint64];
   edge_indexes:[uint32];
}

table SnapBuckets {
    snap_buckets:[SnapBucket];
}
